<table id="products-table" class="Table">
  <thead>
    <tr>
      <th>Produto</th>
      <th>Quantidade</th>
      <th>Preco Unit√°rio</th>
      <th>Preco Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% produtos.each do |produto| %>
      <tr>
        <td><%= produto.produto.nome %></td>
        <td><%= produto.quantidade %></td>
        <td><%= number_to_currency produto.produto.preco %></td>
        <td><%= number_to_currency produto.total %></td>
        <td><a href="" class="Button ButtonDelete delete-product" data-produto-venda-id="<%= produto.id %>">Excluir</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
